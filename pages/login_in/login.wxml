<image class="backimg" src="../../images/back1.png" mode="scaleToFill"></image>
  <view class="container"> 
    <view class="login-icon"> 
      <image class="login-img" src="../../images/logo2.png"></image> 
    </view> 
    <view class="login-from"> 
  <view class="box">
      <!--账号--> 
      <view class="inputView"> 
        <image class="nameImage" src="../../images/usericon.png"></image> 
        <label class="loginLab">  </label> 
        <input class="inputText" placeholder="请输入工号" value="{{username}}" bindinput="usernameinput" /> 
      </view> 
      <view class="line"></view> 
      <!--密码--> 
      <view class="inputView"> 
        <image class="keyImage" src="../../images/passicon.png"></image> 
        <label class="loginLab">  </label> 
        <input class="inputText" password="true" placeholder="请输入密码" value='{{password}}'bindinput="passwordinput" /> 
      </view> 
      <!--按钮--> 
      <view class="loginBtnView"> 
        <button class="loginBtn"bindtap="dologin">登录</button>  
      </view> 
      <view class="loginBtnView"> 
      <button class="loginBtn"bindtap="wx_login">微信登录</button>
      </view> 
    </view> 
  </view>
  </view>


